{
    "name": "react-customizable-progressbar",
    "version": "1.0.1",
    "main": "./dist/ReactCustomizableProgressbar.js",
    "files": [
        "dist"
    ],
    "scripts": {
        "format": "prettier --write {src,examples}/**/*.{js,jsx}",
        "dev": "webpack-dev-server --content-base src --inline --config webpack.config.dev.js",
        "prod": "node ./node_modules/webpack/bin/webpack.js -p --progress --config ./webpack.config.prod.js"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/martyan/react-customizable-progressbar.git"
    },
    "description": "Customizable circular SVG progress bar component for React",
    "keywords": [
        "react",
        "circular",
        "progress",
        "bar",
        "component",
        "customizable",
        "svg",
        "animation"
    ],
    "homepage": "https://martinjuzl.com/react-customizable-progressbar",
    "author": "Martin Juzl",
    "license": "MIT",
    "devDependencies": {
        "@babel/core": "^7.0.0",
        "@babel/plugin-proposal-class-properties": "^7.0.0",
        "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
        "@babel/polyfill": "^7.0.0",
        "@babel/preset-env": "^7.0.0",
        "@babel/preset-react": "^7.0.0",
        "autoprefixer": "^9.1.5",
        "babel-eslint": "^9.0.0",
        "babel-loader": "^8.0.2",
        "babel-plugin-react-css-modules": "^3.4.2",
        "clean-webpack-plugin": "^0.1.19",
        "css-loader": "^1.0.0",
        "eslint": "^5.6.0",
        "eslint-loader": "^2.1.1",
        "eslint-plugin-react": "^7.11.1",
        "html-webpack-plugin": "^3.2.0",
        "moment": "^2.22.2",
        "node-sass": "^4.13.1",
        "postcss-loader": "^3.0.0",
        "postcss-scss": "^2.0.0",
        "prettier": "^1.16.4",
        "prop-types": "^15.6.2",
        "react": "^16.5.2",
        "react-dom": "^16.5.2",
        "react-hot-loader": "^3.0.0-beta.7",
        "sass-loader": "^7.1.0",
        "style-loader": "^0.23.0",
        "uglifyjs-webpack-plugin": "^2.0.1",
        "webpack": "^4.20.2",
        "webpack-cli": "^3.1.1",
        "webpack-dev-middleware": "^3.4.0",
        "webpack-dev-server": "^3.1.14"
