{
  "name": "@storex/react",
  "scope": "@storex",
  "version": "1.1.10",
  "description": "Binding store to your app",
  "main": "lib/index.js",
  "publishConfig": {
    "registry": "https://registry.npmjs.org",
    "access": "public"
  },
  "scripts": {
    "test": "jest",
    "build": "tsc",
    "update-dependence": "npm update @storex/core",
    "release": "npm run update-dependence && rm -rf lib && tsc --sourceMap false && npm test && npm publish --access=public && npm version patch"
  },
  "author": "Moshe Kabala",
  "license": "MIT",
  "devDependencies": {
    "@types/enzyme": "^3.1.12",
    "@types/enzyme-adapter-react-16": "^1.0.2",
    "@types/react": "^16.8.19",
    "@types/react-dom": "^16.0.6",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "typescript": "^3.8.2"
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "ts-jest"
    },
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/lib/",
      "/src/"
    ],
    "moduleNameMapper": {
      "@/(.*)$": "<rootDir>/src/$1"
    },
    "testRegex": "\\.(test|spec)\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ]
  },
  "keywords": [
    "store",
    "state",
    "common state",
    "redux",
    "flux",
    "react",
    "stencil",
    "alt",
    "observer",
    "observable",
    "mobx-state-tree"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/moshe-kabala/StoreX"
  },
  "dependencies": {
    "@storex/core": "^1.1.10",
    "@storex/utils": "^1.1.10",
    "@types/jest": "^25.1.3",
    "react": "^16.8.6",
    "react-dom": "^16.8.6"
  },
  "gitHead": "324136937d52443f76759c067599fe6d885ba894"
}
