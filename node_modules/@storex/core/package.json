{
  "name": "@storex/core",
  "version": "1.1.10",
  "description": "Binding store to your app",
  "main": "lib/index.js",
  "scope": "@storex",
  "scripts": {
    "test": "jest",
    "build": "tsc",
    "release": "rm -rf lib && tsc --sourceMap false && npm test && npm publish --access=public && npm version patch"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org",
    "access": "public"
  },
  "author": "Moshe Kabala",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^23.1.4",
    "jest": "^23.3.0",
    "ts-jest": "^23.0.0",
    "typescript": "^3.8.2"
  },
  "keywords": [
    "@storex/core",
    "storex",
    "store",
    "state",
    "common state",
    "redux",
    "flux",
    "react",
    "stencil",
    "alt",
    "observer",
    "observable",
    "mobx-state-tree"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/moshe-kabala/StoreX"
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "ts-jest"
    },
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/lib/"
    ],
    "moduleNameMapper": {
      "@/(.*)$": "<rootDir>/src/$1"
    },
    "testRegex": "\\.(test|spec)\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ]
  },
  "dependencies": {
    "@storex/utils": "^1.1.10"
  },
  "gitHead": "324136937d52443f76759c067599fe6d885ba894"
}
